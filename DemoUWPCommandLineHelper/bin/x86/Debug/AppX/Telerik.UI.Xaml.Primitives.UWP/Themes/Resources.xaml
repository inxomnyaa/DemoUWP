                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_SelectedPathElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_ArrowElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_PointerOverPathElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Path x:Name="PART_ArrowElement" Canvas.ZIndex="100" Fill="{TemplateBinding Background}" StrokeThickness="0" Visibility="Collapsed"/>
                        <Path x:Name="PART_PointerOverPathElement" Fill="{TemplateBinding Background}" StrokeThickness="0"  Canvas.ZIndex="100" Visibility="Collapsed"/>
                        <Path x:Name="PART_SelectedPathElement" Fill="{TemplateBinding Background}" StrokeThickness="0"  Canvas.ZIndex="100" Visibility="Collapsed"/>
