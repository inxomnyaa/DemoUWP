    <Style TargetType="localRadialMenu:DecorationItemButton">
        <Setter Property="Background" Value="{ThemeResource TelerikRadialMenuStateItemButtonPointerOverBackgroundBrush}"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
