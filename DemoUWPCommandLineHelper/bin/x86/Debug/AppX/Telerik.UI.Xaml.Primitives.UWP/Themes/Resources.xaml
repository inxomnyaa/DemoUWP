                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{ThemeResource TelerikRadialMenuStateItemButtonSelectedBackgroundBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="SelectedPointerOver">
